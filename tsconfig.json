{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "preserve",
    "jsxImportSource": "repeater14",
    "moduleResolution": "node",
    //"types": ["vite/client"],
    "noEmit": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  }
  // "exclude": ["node_modules"]
}
