{
  "extends": ["../../../tsconfig.json"],
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "noEmit": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./src"
  },
  "include": ["src", "./vite.config.ts"]
}
